<%= form_for @encounter do |f| %>

<%= f.label :cryptid %><br>
<%= f.collection_select  :cryptid_id, Cryptid.all, :id, :name %><br>

<%= f.label :location, 'Location' %><br>
<%= f.text_field :location %><br>

<%= f.label :date, 'Date' %><br>
<%= f.date_field :date %><br>

<%= f.label :story, 'Story' %><br>
<%= f.text_area :story %><br>



<%= f.hidden_field :user_id, value: 1 %>
<%= f.submit %>
    
<% end %>